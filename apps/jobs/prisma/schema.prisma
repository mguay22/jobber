generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-1.1.x"]
    output = "../../../node_modules/@prisma-clients/jobs"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Job {
   id Int @default(autoincrement()) @id
   name String
   size Int
   completed Int
   status String
   started DateTime @default(now())
   ended DateTime?
}
