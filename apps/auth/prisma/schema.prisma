generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-1.1.x"]
    output = "../../../node_modules/@prisma-clients/auth"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User {
    id Int @default(autoincrement()) @id
    email String @unique
    password String
}
